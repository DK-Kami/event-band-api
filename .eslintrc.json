{
  "plugins": [
    "babel",
    "jest"
  ],
  "env": {
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "rules": {
    "no-console": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "jest/no-disabled-tests": 0
  }
}
